<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:PersonalExpenses.Pages"
             xmlns:local1="clr-namespace:PersonalExpenses"
             x:Class="PersonalExpenses.Pages.TabbedPageT"
             xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
             android:TabbedPage.ToolbarPlacement="Bottom"
             Title="TabbedPageT">
    
    <NavigationPage Title="Inicio"
                    IconImageSource="home.png">
        <x:Arguments>
            <local1:MainPage/>
        </x:Arguments>
    </NavigationPage>

    <NavigationPage Title="Categorias"
                    IconImageSource="categoria.png">
        <x:Arguments >
            <local:AdministrarCategorias/>
        </x:Arguments>
    </NavigationPage>

    

</TabbedPage>